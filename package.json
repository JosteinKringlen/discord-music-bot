    "@discordjs/voice": "^0.10.0",
    "discord.js": "^13.8.0",
    "discordjs-button-pagination": "^3.0.0",
    "dotenv": "^16.0.1",
    "libsodium-wrappers": "^0.7.10",
    "play-dl": "^1.9.5",
    "tiny-invariant": "^1.2.0",
    "winston": "^3.7.2",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@types/node": "17.0.41",
    "@typescript-eslint/eslint-plugin": "5.30.3",
